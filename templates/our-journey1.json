{
  "colors": {
    "brand-primary": "#000000",
    "brand-secondary": "#F3F4F6",
    "brand-accent": "#C7007D",
    "brand-text-primary": "#FFFFFF",
    "brand-text-secondary": "#E5E7EB",
    "brand-background": "#111111",
    "brand-highlight": "#C7007D"
  },
  "styleVariables": {
    "brandPrimary": "#000000",
    "brandSecondary": "#F3F4F6",
    "brandAccent": "#C7007D",
    "brandTextPrimary": "#FFFFFF",
    "brandTextSecondary": "#E5E7EB",
    "brandBackground": "#111111",
    "brandHighlight": "#C7007D",
    "fontFamilySerif": "'Lora', serif",
    "fontFamilyDisplay": "'Playfair Display', serif"
  },
  "metadata": {
    "name": "Our Journey",
    "description": "An artisan brand story page with hero, process timeline, before/after showcase, testimonials, awards, and a call-to-action.",
    "template": "story-journey",
    "version": "1.0",
    "features": ["animations", "interactive", "special-components"]
  },
  "animations": {
    "fadeInUp": {
      "keyframes": {
        "0%": { "opacity": "0", "transform": "translateY(30px)" },
        "100%": { "opacity": "1", "transform": "translateY(0)" }
      },
      "duration": "0.8s",
      "easing": "cubic-bezier(0.4, 0, 0.2, 1)",
      "fillMode": "both"
    },
    "slideInLeft": {
      "keyframes": {
        "0%": { "opacity": "0", "transform": "translateX(-50px)" },
        "100%": { "opacity": "1", "transform": "translateX(0)" }
      },
      "duration": "0.6s",
      "easing": "ease-out",
      "fillMode": "both"
    },
    "slideInRight": {
      "keyframes": {
        "0%": { "opacity": "0", "transform": "translateX(50px)" },
        "100%": { "opacity": "1", "transform": "translateX(0)" }
      },
      "duration": "0.6s",
      "easing": "ease-out",
      "fillMode": "both"
    }
  },
  "component": {
    "id": "root",
    "type": "div",
    "props": {
      "className": "container mx-auto px-4 py-16 antialiased",
      "style": {
        "backgroundColor": "var(--brandBackground)",
        "color": "var(--brandTextPrimary)",
        "fontFamily": "var(--fontFamilySerif)"
      }
    },
    "children": [
      {
        "id": "hero-section",
        "type": "section",
        "props": {
          "className": "text-center mb-24 handcrafted-texture rounded-lg py-20 px-6",
          "style": {
            "backgroundColor": "var(--brandPrimary)",
            "color": "var(--brandTextPrimary)",
            "fontFamily": "var(--fontFamilyDisplay)"
          }
        },
        "children": [
          {
            "id": "hero-title",
            "type": "h1",
            "props": {
              "className": "text-5xl md:text-6xl font-bold animate-fadeInUp mb-4",
              "style": {
                "color": "var(--brandTextPrimary)",
                "fontFamily": "var(--fontFamilyDisplay)"
              }
            },
            "children": ["{{content.basics.name || 'Layered Artistry'}}"],
            "contentKey": "basics.name",
            "editable": { "contentEditable": true }
          },
          {
            "id": "hero-subtitle",
            "type": "p",
            "props": {
              "className": "text-xl max-w-3xl mx-auto mb-10 animate-fadeInUp",
              "style": {
                "color": "var(--brandTextSecondary)",
                "fontFamily": "var(--fontFamilySerif)"
              }
            },
            "children": ["{{content.basics.value || 'The Immersive Journey of Craft'}}"],
            "contentKey": "basics.value",
            "editable": { "contentEditable": true }
          },
          {
            "id": "hero-ctas",
            "type": "div",
            "props": { "className": "flex justify-center gap-4 animate-fadeInUp" },
            "children": [
              {
                "id": "cta-discover",
                "type": "a",
                "props": {
                  "href": "#",
                  "className": "bg-[var(--brandAccent)] text-white font-semibold py-3 px-8 rounded-md shadow-lg hover:bg-opacity-90 transition-all duration-300 transform hover:scale-105"
                },
                "children": ["Discover Our Process"]
              },
              {
                "id": "cta-learn",
                "type": "a",
                "props": {
                  "href": "#",
                  "className": "bg-transparent border-2 border-[var(--brandAccent)] text-[var(--brandAccent)] font-semibold py-3 px-8 rounded-md hover:bg-[var(--brandAccent)] hover:text-white transition-all duration-300 transform hover:scale-105 hover:shadow-md"
                },
                "children": ["Learn More"]
              }
            ]
          }
        ]
      },
      {
        "id": "process-header",
        "type": "header",
        "props": { "className": "text-center mb-16 animate-fadeInUp" },
        "children": [
          {
            "id": "process-kicker",
            "type": "span",
            "props": {
              "className": "inline-block bg-[var(--brandAccent)] bg-opacity-20 text-[var(--brandAccent)] text-sm font-semibold px-4 py-2 rounded-full mb-4 tracking-wider"
            },
            "children": ["Our Process"]
          },
          {
            "id": "process-title",
            "type": "h2",
            "props": {
              "className": "text-4xl md:text-5xl font-bold",
              "style": {
                "color": "var(--brandTextPrimary)",
                "fontFamily": "var(--fontFamilyDisplay)"
              }
            },
            "children": ["{{'From Idea to Innovation'}}"],
            "contentKey": "story.solution"
          }
        ]
      },
      {
        "id": "timeline",
        "type": "div",
        "props": { "className": "relative max-w-4xl mx-auto" },
        "children": [
          {
            "id": "timeline-line",
            "type": "div",
            "props": {
              "className": "absolute left-1/2 -ml-0.5 w-1 bg-[var(--brandAccent)] bg-opacity-30 h-full"
            }
          },
          {
            "id": "timeline-item-1",
            "type": "div",
            "props": { "className": "mb-12 flex justify-between items-center w-full" },
            "children": [
              { "id": "t1-spacer", "type": "div", "props": { "className": "order-1 w-5/12" } },
              {
                "id": "t1-badge",
                "type": "div",
                "props": {
                  "className": "z-10 flex items-center order-1 bg-[var(--brandAccent)] shadow-xl w-14 h-14 rounded-full"
                },
                "children": [
                  { "id": "t1-number", "type": "h1", "props": { "className": "mx-auto font-semibold text-2xl text-white" }, "children": ["1"] }
                ]
              },
              {
                "id": "t1-card",
                "type": "div",
                "props": {
                  "className": "order-1 bg-[var(--brandPrimary)] rounded-lg shadow-xl w-5/12 px-6 py-4 border-l-4 border-[var(--brandAccent)] animate-slideInRight"
                },
                "children": [
                  { "id": "t1-title", "type": "h3", "props": { "className": "font-bold text-[var(--brandTextPrimary)] text-xl", "style": { "fontFamily": "var(--fontFamilyDisplay)" } }, "children": ["The Spark of Creation"] },
                  {
                    "id": "t1-text",
                    "type": "p",
                    "props": { "className": "text-base leading-snug tracking-wide text-[var(--brandTextSecondary)] mt-2", "style": { "fontFamily": "var(--fontFamilySerif)" } },
                    "children": ["{{content.process.creation || 'Where ideas are born from passion and sketches take their first breath.'}}"],
                    "contentKey": "process.creation"
                  }
                ]
              }
            ]
          },
          {
            "id": "timeline-item-2",
            "type": "div",
            "props": { "className": "mb-12 flex justify-between flex-row-reverse items-center w-full" },
            "children": [
              { "id": "t2-spacer", "type": "div", "props": { "className": "order-1 w-5/12" } },
              {
                "id": "t2-badge",
                "type": "div",
                "props": { "className": "z-10 flex items-center order-1 bg-[var(--brandAccent)] shadow-xl w-14 h-14 rounded-full" },
                "children": [
                  { "id": "t2-number", "type": "h1", "props": { "className": "mx-auto text-white font-semibold text-2xl" }, "children": ["2"] }
                ]
              },
              {
                "id": "t2-card",
                "type": "div",
                "props": {
                  "className": "order-1 bg-[var(--brandPrimary)] rounded-lg shadow-xl w-5/12 px-6 py-4 border-r-4 border-[var(--brandAccent)] animate-slideInLeft"
                },
                "children": [
                  { "id": "t2-title", "type": "h3", "props": { "className": "font-bold text-[var(--brandTextPrimary)] text-xl", "style": { "fontFamily": "var(--fontFamilyDisplay)" } }, "children": ["Sourcing Quality Materials"] },
                  {
                    "id": "t2-text",
                    "type": "p",
                    "props": { "className": "text-base leading-snug tracking-wide text-[var(--brandTextSecondary)] mt-2", "style": { "fontFamily": "var(--fontFamilySerif)" } },
                    "children": ["{{content.process.materials || 'Only the finest, ethically sourced materials are chosen for our creations.'}}"],
                    "contentKey": "process.materials"
                  }
                ]
              }
            ]
          },
          {
            "id": "timeline-item-3",
            "type": "div",
            "props": { "className": "mb-12 flex justify-between items-center w-full" },
            "children": [
              { "id": "t3-spacer", "type": "div", "props": { "className": "order-1 w-5/12" } },
              {
                "id": "t3-badge",
                "type": "div",
                "props": { "className": "z-10 flex items-center order-1 bg-[var(--brandAccent)] shadow-xl w-14 h-14 rounded-full" },
                "children": [
                  { "id": "t3-number", "type": "h1", "props": { "className": "mx-auto font-semibold text-2xl text-white" }, "children": ["3"] }
                ]
              },
              {
                "id": "t3-card",
                "type": "div",
                "props": { "className": "order-1 bg-[var(--brandPrimary)] rounded-lg shadow-xl w-5/12 px-6 py-4 border-l-4 border-[var(--brandAccent)] animate-slideInRight" },
                "children": [
                  { "id": "t3-title", "type": "h3", "props": { "className": "font-bold text-[var(--brandTextPrimary)] text-xl", "style": { "fontFamily": "var(--fontFamilyDisplay)" } }, "children": ["Time-Honored Expertise"] },
                  {
                    "id": "t3-text",
                    "type": "p",
                    "props": { "className": "text-base leading-snug tracking-wide text-[var(--brandTextSecondary)] mt-2", "style": { "fontFamily": "var(--fontFamilySerif)" } },
                    "children": ["{{content.process.time || 'Our hands shape and refine, pouring years of skill into every detail.'}}"],
                    "contentKey": "process.time"
                  }
                ]
              }
            ]
          },
          {
            "id": "timeline-item-4",
            "type": "div",
            "props": { "className": "mb-12 flex justify-between flex-row-reverse items-center w-full" },
            "children": [
              { "id": "t4-spacer", "type": "div", "props": { "className": "order-1 w-5/12" } },
              {
                "id": "t4-badge",
                "type": "div",
                "props": { "className": "z-10 flex items-center order-1 bg-[var(--brandAccent)] shadow-xl w-14 h-14 rounded-full" },
                "children": [
                  { "id": "t4-number", "type": "h1", "props": { "className": "mx-auto text-white font-semibold text-2xl" }, "children": ["4"] }
                ]
              },
              {
                "id": "t4-card",
                "type": "div",
                "props": { "className": "order-1 bg-[var(--brandPrimary)] rounded-lg shadow-xl w-5/12 px-6 py-4 border-r-4 border-[var(--brandAccent)] animate-slideInLeft" },
                "children": [
                  { "id": "t4-title", "type": "h3", "props": { "className": "font-bold text-[var(--brandTextPrimary)] text-xl", "style": { "fontFamily": "var(--fontFamilyDisplay)" } }, "children": ["Artisanal Assurance"] },
                  {
                    "id": "t4-text",
                    "type": "p",
                    "props": { "className": "text-base leading-snug tracking-wide text-[var(--brandTextSecondary)] mt-2", "style": { "fontFamily": "var(--fontFamilySerif)" } },
                    "children": ["{{content.process.quality || 'Every piece is inspected with a craftsman's eye for perfection.'}}"],
                    "contentKey": "process.quality"
                  }
                ]
              }
            ]
          },
          {
            "id": "timeline-item-5",
            "type": "div",
            "props": { "className": "flex justify-between items-center w-full" },
            "children": [
              { "id": "t5-spacer", "type": "div", "props": { "className": "order-1 w-5/12" } },
              {
                "id": "t5-badge",
                "type": "div",
                "props": { "className": "z-10 flex items-center order-1 bg-[var(--brandAccent)] shadow-xl w-14 h-14 rounded-full" },
                "children": [
                  { "id": "t5-number", "type": "h1", "props": { "className": "mx-auto font-semibold text-2xl text-white" }, "children": ["5"] }
                ]
              },
              {
                "id": "t5-card",
                "type": "div",
                "props": { "className": "order-1 bg-[var(--brandPrimary)] rounded-lg shadow-xl w-5/12 px-6 py-4 border-l-4 border-[var(--brandAccent)] animate-slideInRight" },
                "children": [
                  { "id": "t5-title", "type": "h3", "props": { "className": "font-bold text-[var(--brandTextPrimary)] text-xl", "style": { "fontFamily": "var(--fontFamilyDisplay)" } }, "children": ["Sustainable Soul"] },
                  {
                    "id": "t5-text",
                    "type": "p",
                    "props": { "className": "text-base leading-snug tracking-wide text-[var(--brandTextSecondary)] mt-2", "style": { "fontFamily": "var(--fontFamilySerif)" } },
                    "children": ["{{content.process.ethics || 'We believe in practices that respect our planet and our community.'}}"],
                    "contentKey": "process.ethics"
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "action-section",
        "type": "section",
        "props": { "className": "mt-24" },
        "children": [
          {
            "id": "action-header",
            "type": "header",
            "props": { "className": "text-center mb-12 animate-fadeInUp" },
            "children": [
              {
                "id": "action-title",
                "type": "h2",
                "props": { "className": "text-4xl md:text-5xl font-bold", "style": { "color": "var(--brandTextPrimary)", "fontFamily": "var(--fontFamilyDisplay)" } },
                "children": ["In Action: Real Life, Real Results"]
              },
              {
                "id": "action-subtitle",
                "type": "p",
                "props": { "className": "text-xl", "style": { "color": "var(--brandTextSecondary)", "fontFamily": "var(--fontFamilySerif)" } },
                "children": ["{{content.impact.cases[0].title || 'From Raw Beauty to Refined Artistry'}}"],
                "contentKey": "impact.cases[0].title"
              },
              {
                "id": "action-description",
                "type": "p",
                "props": { "className": "text-base opacity-80 mt-4 max-w-2xl mx-auto", "style": { "color": "var(--brandTextSecondary)", "fontFamily": "var(--fontFamilySerif)" } },
                "children": ["{{content.impact.cases[0].description || 'Witness the evolution of our craft. Our interactive slider allows you to compare the initial, raw material with the final, refined piece. Slide to reveal the journey of transformation and the meticulous artistry involved.'}}"],
                "contentKey": "impact.cases[0].description"
              }
            ]
          },
          {
            "id": "action-body",
            "type": "div",
            "props": { "className": "max-w-4xl mx-auto p-8 rounded-lg shadow-xl border-t-4 animate-fadeInUp", "style": { "backgroundColor": "var(--brandPrimary)", "borderTopColor": "var(--brandAccent)", "fontFamily": "var(--fontFamilySerif)" } },
            "children": [
              {
                "id": "before-after-gallery",
                "type": "div",
                "props": { "className": "w-full" },
                "children": "BEFORE_AFTER_GALLERY"
              }
            ]
          }
        ]
      },
      {
        "id": "success-section",
        "type": "section",
        "if": "content.impact.testimonials && content.impact.testimonials.length > 0",
        "props": { "className": "mt-24 text-center" },
        "children": [
          {
            "id": "success-header",
            "type": "header",
            "props": { "className": "mb-12 animate-fadeInUp" },
            "children": [
              {
                "id": "success-title",
                "type": "h2",
                "props": { "className": "text-4xl md:text-5xl font-bold", "style": { "color": "var(--brandTextPrimary)", "fontFamily": "var(--fontFamilyDisplay)" } },
                "children": ["{{content.success.title || 'Success Stories'}}"],
                "contentKey": "success.title"
              },
              {
                "id": "success-subtitle",
                "type": "p",
                "props": { "className": "text-xl", "style": { "color": "var(--brandTextSecondary)", "fontFamily": "var(--fontFamilySerif)" } },
                "children": ["Celebrated by the Community"],
                "contentKey": "success.subtitle"
              }
            ]
          },
          {
            "id": "success-grid",
            "type": "div",
            "props": { "className": "flex flex-wrap justify-center gap-8" },
            "children": "TESTIMONIALS_ARRAY"
          }
        ]
      },
      {
        "id": "awards-section",
        "type": "section",
        "if": "content.impact.awards && content.impact.awards.length > 0",
        "props": { "className": "mt-24 p-12 rounded-lg shadow-xl handcrafted-texture border-t-4", "style": { "backgroundColor": "var(--brandPrimary)", "borderTopColor": "var(--brandAccent)", "color": "var(--brandTextPrimary)" } },
        "children": [
          {
            "id": "awards-header",
            "type": "header",
            "props": { "className": "text-center mb-12 animate-fadeInUp" },
            "children": [
              {
                "id": "awards-title",
                "type": "h2",
                "props": { "className": "text-4xl md:text-5xl font-bold", "style": { "color": "var(--brandTextPrimary)", "fontFamily": "var(--fontFamilyDisplay)" } },
                "children": ["{{content.awards.title || 'Recognition & Awards'}}"],
                "contentKey": "awards.title"
              },
              {
                "id": "awards-subtitle",
                "type": "p",
                "props": { "className": "text-xl", "style": { "color": "var(--brandTextSecondary)", "fontFamily": "var(--fontFamilySerif)" } },
                "children": ["Celebrated by the Community"],
                "contentKey": "awards.subtitle"
              }
            ]
          },
          {
            "id": "awards-list",
            "type": "div",
            "props": { "className": "max-w-4xl mx-auto divide-y" },
            "children": "AWARDS_ARRAY"
          }
        ]
      },
      {
        "id": "cta-section",
        "type": "section",
        "props": { "className": "mt-24 rounded-lg p-12 text-center handcrafted-texture", "style": { "backgroundColor": "var(--brandAccent)", "color": "var(--brandTextPrimary)" } },
        "children": [
          { "id": "cta-title", "type": "h2", "props": { "className": "text-4xl font-bold animate-fadeInUp", "style": { "color": "var(--brandTextPrimary)", "fontFamily": "var(--fontFamilyDisplay)" } }, "children": ["Ready to Start Your Own Journey?"] },
          { "id": "cta-text", "type": "p", "props": { "className": "mt-4 max-w-2xl mx-auto text-lg opacity-90 animate-fadeInUp", "style": { "color": "var(--brandTextPrimary)", "fontFamily": "var(--fontFamilySerif)" } }, "children": ["Commission a piece or find the perfect creation that speaks to you."] },
          {
            "id": "cta-buttons",
            "type": "div",
            "props": { "className": "mt-8 flex justify-center gap-4 animate-fadeInUp" },
            "children": [
              { "id": "cta-commission", "type": "button", "props": { "className": "bg-[var(--brandTextPrimary)] font-semibold py-3 px-6 rounded-md shadow-md hover:bg-[var(--brandSecondary)] hover:text-[var(--brandPrimary)] transition-all duration-300 transform hover:scale-105 hover:shadow-lg", "style": { "color": "var(--brandPrimary)" } }, "children": ["Commission a Piece"] },
              { "id": "cta-explore", "type": "button", "props": { "className": "bg-transparent border-2 border-[var(--brandTextPrimary)] font-semibold py-3 px-6 rounded-md hover:bg-[var(--brandTextPrimary)] hover:text-[var(--brandPrimary)] transition-all duration-300 transform hover:scale-105 hover:shadow-md", "style": { "color": "var(--brandTextPrimary)" } }, "children": ["Explore Our Shop"] }
            ]
          }
        ]
      }
    ]
  }
}
