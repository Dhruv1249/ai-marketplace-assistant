{
  "metadata": {
    "template": "seller-info-creative",
    "name": "Creative Seller Info Template",
    "description": "Vibrant, artistic layout for creative professionals",
    "version": "1.0"
  },
  "styleVariables": {
    "primaryColor": "#9333ea",
    "secondaryColor": "#ec4899",
    "backgroundColor": "#fdf4ff",
    "textColor": "#1f2937"
  },
  "component": {
    "id": "creative-seller-root",
    "type": "div",
    "props": {
      "className": "max-w-6xl mx-auto bg-gradient-to-br from-purple-50 via-pink-50 to-indigo-50 min-h-screen"
    },
    "children": [
      {
        "id": "hero-section",
        "type": "div",
        "props": {
          "className": "relative overflow-hidden"
        },
        "children": [
          {
            "id": "hero-background",
            "type": "div",
            "props": {
              "className": "absolute inset-0 bg-gradient-to-r from-purple-600/20 to-pink-600/20"
            }
          },
          {
            "id": "hero-content",
            "type": "div",
            "props": {
              "className": "relative px-8 py-16 text-center"
            },
            "children": [
              {
                "id": "profile-photo-section",
                "type": "div",
                "props": {
                  "className": "relative inline-block mb-6"
                },
                "children": [
                  {
                    "id": "profile-photo",
                    "type": "img",
                    "if": "content.photos && content.photos.length > 0",
                    "props": {
                      "src": "{{content.photos[0].url}}",
                      "alt": "{{content.name}}",
                      "className": "w-40 h-40 rounded-full object-cover border-4 border-white shadow-2xl"
                    }
                  },
                  {
                    "id": "profile-placeholder",
                    "type": "div",
                    "unless": "content.photos && content.photos.length > 0",
                    "props": {
                      "className": "w-40 h-40 rounded-full bg-gradient-to-br from-purple-400 to-pink-400 flex items-center justify-center border-4 border-white shadow-2xl mx-auto"
                    },
                    "children": [
                      {
                        "id": "placeholder-initial",
                        "type": "span",
                        "props": {
                          "className": "text-white text-6xl font-bold"
                        },
                        "children": "{{content.name ? content.name.charAt(0).toUpperCase() : 'U'}}"
                      }
                    ]
                  }
                ]
              },
              {
                "id": "seller-name",
                "type": "h1",
                "editable": {
                  "contentEditable": true
                },
                "props": {
                  "className": "text-4xl md:text-5xl font-bold text-gray-900 mb-4"
                },
                "children": "{{content.name || 'Your Name'}}"
              },
              {
                "id": "seller-title",
                "type": "p",
                "editable": {
                  "contentEditable": true
                },
                "props": {
                  "className": "text-2xl text-purple-600 font-medium mb-6"
                },
                "children": "{{content.title || 'Your Creative Title'}}"
              },
              {
                "id": "seller-bio",
                "type": "p",
                "editable": {
                  "contentEditable": true
                },
                "props": {
                  "className": "text-lg text-gray-700 max-w-3xl mx-auto leading-relaxed"
                },
                "children": "{{content.bio || 'Your creative bio will appear here...'}}"
              }
            ]
          }
        ]
      },
      {
        "id": "content-sections",
        "type": "div",
        "props": {
          "className": "px-8 py-12"
        },
        "children": [
          {
            "id": "content-container",
            "type": "div",
            "props": {
              "className": "max-w-4xl mx-auto space-y-12"
            },
            "children": [
              {
                "id": "story-section",
                "type": "div",
                "if": "content.story",
                "props": {
                  "className": "bg-white/70 backdrop-blur-sm rounded-2xl p-8 shadow-lg"
                },
                "children": [
                  {
                    "id": "story-title",
                    "type": "h2",
                    "props": {
                      "className": "text-3xl font-bold text-gray-900 mb-6 text-center"
                    },
                    "children": "My Creative Journey"
                  },
                  {
                    "id": "story-content",
                    "type": "p",
                    "editable": {
                      "contentEditable": true
                    },
                    "props": {
                      "className": "text-gray-700 leading-relaxed text-lg"
                    },
                    "children": "{{content.story}}"
                  }
                ]
              },
              {
                "id": "skills-achievements-grid",
                "type": "div",
                "props": {
                  "className": "grid grid-cols-1 md:grid-cols-2 gap-8"
                },
                "children": [
                  {
                    "id": "specialties-section",
                    "type": "div",
                    "if": "content.specialties && content.specialties.length > 0",
                    "props": {
                      "className": "bg-white/70 backdrop-blur-sm rounded-2xl p-6 shadow-lg"
                    },
                    "children": [
                      {
                        "id": "specialties-title",
                        "type": "h3",
                        "props": {
                          "className": "text-2xl font-bold text-gray-900 mb-4"
                        },
                        "children": "Creative Skills"
                      },
                      {
                        "id": "specialties-list",
                        "type": "div",
                        "props": {
                          "className": "space-y-3"
                        },
                        "children": "SPECIALTIES_ARRAY"
                      }
                    ]
                  },
                  {
                    "id": "achievements-section",
                    "type": "div",
                    "if": "content.achievements && content.achievements.length > 0",
                    "props": {
                      "className": "bg-white/70 backdrop-blur-sm rounded-2xl p-6 shadow-lg"
                    },
                    "children": [
                      {
                        "id": "achievements-title",
                        "type": "h3",
                        "props": {
                          "className": "text-2xl font-bold text-gray-900 mb-4"
                        },
                        "children": "Achievements"
                      },
                      {
                        "id": "achievements-list",
                        "type": "div",
                        "props": {
                          "className": "space-y-3"
                        },
                        "children": "ACHIEVEMENTS_ARRAY"
                      }
                    ]
                  }
                ]
              },
              {
                "id": "contact-social-section",
                "type": "div",
                "props": {
                  "className": "bg-white/70 backdrop-blur-sm rounded-2xl p-8 shadow-lg text-center"
                },
                "children": [
                  {
                    "id": "connect-title",
                    "type": "h3",
                    "props": {
                      "className": "text-2xl font-bold text-gray-900 mb-6"
                    },
                    "children": "Let's Connect"
                  },
                  {
                    "id": "contact-links",
                    "type": "div",
                    "props": {
                      "className": "flex flex-wrap justify-center gap-6"
                    },
                    "children": [
                      {
                        "id": "email-contact",
                        "type": "div",
                        "if": "content.contact && content.contact.email",
                        "props": {
                          "className": "flex items-center gap-2 text-gray-700"
                        },
                        "children": [
                          {
                            "id": "email-icon",
                            "type": "span",
                            "props": {
                              "className": "w-5 h-5 inline-block text-purple-600"
                            },
                            "children": "📧"
                          },
                          {
                            "id": "email-text",
                            "type": "span",
                            "children": "{{content.contact.email}}"
                          }
                        ]
                      },
                      {
                        "id": "website-contact",
                        "type": "a",
                        "if": "content.contact && content.contact.website",
                        "props": {
                          "href": "{{content.contact.website}}",
                          "className": "flex items-center gap-2 text-purple-600 hover:text-purple-800"
                        },
                        "children": [
                          {
                            "id": "website-icon",
                            "type": "span",
                            "props": {
                              "className": "w-5 h-5 inline-block"
                            },
                            "children": "🌐"
                          },
                          {
                            "id": "website-text",
                            "type": "span",
                            "children": "Portfolio"
                          }
                        ]
                      },
                      {
                        "id": "instagram-contact",
                        "type": "a",
                        "if": "content.contact && content.contact.social && content.contact.social.instagram",
                        "props": {
                          "href": "{{content.contact.social.instagram}}",
                          "className": "flex items-center gap-2 text-pink-600 hover:text-pink-800"
                        },
                        "children": [
                          {
                            "id": "instagram-icon",
                            "type": "span",
                            "props": {
                              "className": "w-5 h-5 inline-block"
                            },
                            "children": "📷"
                          },
                          {
                            "id": "instagram-text",
                            "type": "span",
                            "children": "Instagram"
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}